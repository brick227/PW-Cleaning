	.single-post .subheader,
  	.page-template-page-blog .subheader {
  		margin-top: 0px;
    	padding-top: 30px;
    	padding-bottom: 90px;
  	}
  	
  	.post-text-content h1,
    .single-post .subheader h1,
    .page-template-page-blog .subheader h1 {
		margin-bottom: 0px !important;
      	font-size: 40px;
      	line-height: 40px;
    }
  
  	.post-text-content h2 {
  		font-size: 30px;
      	line-height: 30px;
      	margin-bottom: 15px;
  	}
  	
  	.post-text-content h3,
  	.post-header-text > h3 {
  		font-size: 20px;
      	line-height: 20px;
  	}
  
  	.blog-content__main {
      	padding: 30px;
        padding-bottom: 0px;
    }
  	
  	.post div.mb\+ {
  		margin-bottom: 30px !important;
  	}
  
  	.blog-content__item {
  		padding-top: 30px;
      	padding-bottom: 30px;
  	}
  
  	.post footer .btn {
  		margin-right: 15px !important;
  	}
  
