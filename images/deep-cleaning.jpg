<style type="text/css">
/* blog page css 
 */

.single-post .subheader {
	padding-bottom: 140px;
}

.single-post .subheader h1,
.page-template-page-blog .subheader h1 {
	text-transform: none;
}


.date-circle {
	overflow: hidden;
	display: -webkit-flex;
	display: flex;
	-webkit-align-items: center;
	align-items: center;
	-webkit-justify-content: center;
	justify-content: center;
	-webkit-flex-direction: column;
	flex-direction: column;
	background: #0e9edf;
	width: 70px;
	height: 70px;
  	min-width: 70px;
	min-height: 70px;
	margin-right: 15px;
	-webkit-border-radius: 70px;
	border-radius: 70px;
	color: white;
  	
}

.date-circle b,
.date-circle span {
	display: block;
}

.date-circle b {
	font-size: 30px;
	line-height: 30px;
}

.date-circle span {
	text-transform: uppercase;
	font-size: 16px;
	line-height: 16px;
}

.post > header {
	border-bottom: none;
	margin-bottom: 30px;
	position: relative;
	display: -webkit-flex;
	display: flex;
	-webkit-justify-content: flex-start;
	justify-content: flex-start;;
}


.post-header-text .flag__img {
	display: none;
}

.post-header-text {
	width: 100%;
}

.post-header-text > h3{
	text-transform: uppercase;
	font-size: 30px;
	line-height: 30px;
}

.post-header-text a:hover {
	text-decoration: none;
	color: #0e9edf;
}

.post footer {
	margin-top: 30px;
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.post footer .btn {
  	border: #0e9edf solid 1px !important;;
  	background: transparent !important;
	max-width: 150px;
  	color: #0e9edf !important;
	width: 100%;
}

.post footer .btn:focus,
.post footer .btn:active,
.post footer .btn:hover {
	background: #0e9edf !important;
  	color: white !important;
}

.post footer .social-bar {
	margin-bottom: 0px;
}

.blog-content__aside .widget_search .flag__img {
	display: none !important;
}

.blog-content__aside .widget {
	margin-bottom: 40px;
}

.blog-content__aside .widget_search {
	position: relative;
	margin-bottom: 20px;
}

.blog-content__aside .icon-search {
	position: absolute;
	right: 10px;
	top: 10px;
}

.side-bar-book-now {
	padding: 30px 20px;
	text-align: center;
	background: url('/wp-content/themes/bluedream/images/patern-blue-new.jpg');
	background-size: cover;
}
.page-template-default .subheader .subheader__content {
    width: 100%;
    max-width: 700px;
    margin: 0px auto;
    padding: 5% 0px;
}

.side-bar-book-now .btn:active,
.side-bar-book-now .btn:focus,
.side-bar-book-now .btn:hover {
	background: white !important;
	color: rgb(21, 48, 72) !important;
}

.side-bar-book-now h3,
.side-bar-book-now p {
	color: white;
}

.side-bar-book-now h3 {
	font-weight: 600;
  	font-size: 30px;
  	line-height: 30px;
}

.side-bar-book-now p {
	margin-bottom: 20px;
}

.blog-content__aside .cat-item {
	list-style: none;
	display: flex;
	align-items: center;
	margin-top: 15px;
}

.blog-content__aside .tag-cloud-link::before,
.blog-content__aside .cat-item::before {
	font-family: 'fontawesome';
	content: '\f054';
	color: #fff;
	background: rgb(21, 48, 72);
	font-size: 9px;
	display: inline-flex;
	align-items: center;
	justify-content: center;
	width: 15px;
	height: 15px;
	-webkit-border-radius: 20px;
	border-radius: 20px;
	margin-right: 10px;
	-webkit-transition: all .3s ease-out;
	-moz-transition: all .3s ease-out;
	-ms-transition: all .3s ease-out;
	transition: all .3s ease-out;
}

.blog-content__aside .cat-item a {
	color: inherit;
}

.blog-content__aside .cat-item:hover:before {
	background: #0e9edf;
}

.blog-content__aside .cat-item:hover a {
	text-decoration: none;
	color: #0e9edf;
}

.blog-content__aside .widget_recent_entries ul,
.blog-content__aside .widget_recent_entries li {
	list-style: none;
}

.blog-content__aside .widget_recent_entries li {
	padding-top: 15px;
	padding-bottom: 15px;
	border-bottom: #ebebeb solid 1px;
}


.blog-content__aside .widget_recent_entries li a {
	display: block;
	color: rgb(21, 48, 72);
	text-transform: uppercase;
}

.blog-content__aside .widget_recent_entries li span {
	display: block;
	font-size: 15px;
	opacity: 0.5;
}


.blog-content__aside .widget_recent_entries li a:hover,
.blog-content__aside .widget_recent_entries li a:focus,
.blog-content__aside .widget_recent_entries li a:active {
	color: #0e9edf;
	text-decoration: none;
}

.blog-content__aside .widget-title {
	text-transform: uppercase;
	font-size: 25px;
}

.blog-content__aside .tagcloud {
	margin-top: 15px;
}

.blog-content__aside .tag-cloud-link {
	border: rgba(0,0,0,0.1) solid 1px;
	height: 36px;
	line-height: 36px;
	display: inline-block;
	padding: 0px 10px;
	color: inherit;
	margin-right: 10px;
	margin-bottom: 10px;
	white-space: nowrap;
	max-width: 100%;
	overflow: hidden;
	text-overflow: ellipsis;
	font-size: 14px !important;
	-webkit-transition: .3s ease-out;
	-moz-transition: .3s ease-out;
	-ms-transition: .3s ease-out;
	-o-transition: .3s ease-out;
	transition: .3s ease-out;
}

.blog-content__aside .tag-cloud-link:active,
.blog-content__aside .tag-cloud-link:focus,
.blog-content__aside .tag-cloud-link:hover {
	color: #0e9edf;
	text-decoration: none;
	border-color: #0e9edf;
}


.blog-content__aside .tag-cloud-link:hover:before {
	background: #0e9edf;
}

@media only screen and (max-width: 900px) {
  	.side-bar-book-now .btn,
    .post footer .btn {
        padding: 0px 10px !important;
        height: 40px !important;
        line-height: 40px !important;
    }
  
  	.post footer .btn {
  		 max-width: 100px;
  	}
  
    .blog-content__main {
      	padding: 40px;
        padding-bottom: 0px;
    }
  
  	.blog-content__item {
  		padding-top: 40px;
      	padding-bottom: 40px;
  	}
  
  	.post div.mb\+ {
  		margin-bottom: 40px !important;
  	}
  
  	.blog-content__aside {
  		padding: 20px;
  	}

}

@media only screen and (max-width: 768px) {
  	.blog-content__aside {
  		padding-left: 0px;
      	padding-right: 0px;
      	padding-bottom: 0px;
  	}
}

@media only screen and (max-width: 640px) {
	.post-header-text > h3 {
  		font-size: 25px;
      	line-height: 25px;
  	}
}

@media only screen and (max-width: 560px) {
    .date-circle {
		width: 60px;
      	height: 60px;
      	min-height: 60px;
      	min-width: 60px;
    }
  
  	.date-circle b {
  		font-size: 25px;
      	line-height: 25px;
  	}
  
  	.date-circle span {
  		font-size: 14px;
      	line-height: 14px;
  	}
}

